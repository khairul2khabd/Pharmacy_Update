<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.pharmacy.model">
    <class name="PhaPatientSearch" table="patient_search" batch-size="500">

        <id name="patientId" type="int" column="patient_id"
            unsaved-value="0">				
        </id>		
        <property name="identifier" column="identifier" />
        <property name="fullname" column="fullname" />
        <property name="givenName" column="given_name" />
        <property name="middleName" column="middle_name" />
        <property name="familyName" column="family_name" />
        <property name="gender" column="gender" />
        <property name="birthdate" column="birthdate" />
        <property name="age" column="age" />
        <property name="personNameId" column="person_name_id" />
        <property name="phoneNo" column="phone_no" />
    </class>
</hibernate-mapping>
